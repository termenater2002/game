MainUI {
    flex-grow: 1;
    background-color: var(--appui-backgrounds-50);
}

.muse-root {
    flex-grow: 1;
    flex-shrink: 0;
}

.muse-overlay {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}

.muse-canvas-container {
    flex-grow: 1;
    position: relative;
}
