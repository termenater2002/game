.deeppose-takes-v2m {
    flex-direction: column;
    flex-grow: 0;
    flex-shrink: 0;
    width: 100%;
    min-height: auto;
}

.deeppose-takes-v2m-browse {
    flex-grow: 1;
    flex-shrink: 1;
    height: auto;
    width: 100%;
}

.deeppose-takes-v2m-video-container {
    width: 100%;
    flex-grow: 1;
    flex-shrink: 1;
    justify-content: center;
    align-items: center;
}

.deeppose-takes-v2m-video-preview {
    height: 232px;
    width: 232px;
    border-radius: var(--appui-sizing-100);
    background-color: #000000;
}

.deeppose-takes-v2m-generate-button {
    flex-grow: 1;
    flex-shrink: 1;
    height: auto;
    width: 100%;
}