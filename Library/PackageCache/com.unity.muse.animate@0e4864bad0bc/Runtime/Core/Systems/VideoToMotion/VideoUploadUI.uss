#v2m-upload-container {
    display: flex;
    width: 100%;
    height: 232px;
    padding: 8px;
    border-radius: var(--appui-sizing-100);
    border-width: 1px;
    border-color: var(--appui-backgrounds-50);
    background-color: var(--appui-backgrounds-100);
    justify-content: center;
    align-items: center;
}

#v2m-upload-container > Text {
    color: var(--appui-alias-text-disabled);
    -unity-text-align: middle-center;
}