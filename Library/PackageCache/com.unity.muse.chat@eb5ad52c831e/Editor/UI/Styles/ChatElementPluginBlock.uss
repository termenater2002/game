.mui-plugin-block-root {
    border-width: 1px;
    border-radius: 8px;
    border-color: var(--muse-chat-backgrounds-50);
    margin-bottom: 20px;
    background-color: var(--muse-chat-backgrounds-100);
    cursor: text;
    justify-content: center;
    padding-left: 12px;
    padding-right: 12px;
    padding-top: 12px;
    padding-bottom: 6px;
}

.mui-plugin-block-text {
    -unity-font-style: bold;
    margin-bottom: 6px;
}
