:root {
    --muse-chat-warning-25: #9d5500;
    --muse-chat-color-dark-gray-800: #727272;
    --muse-chat-base-200: #4f4f4f;
    --muse-chat-primary-50: #202020;
    --muse-chat-primary-500: #5d5d5d;
    --muse-chat-primary-800: #a2a2a2;
    --muse-chat-primary-1200: #e9e9e9;
    --muse-chat-text-disabled: #858585;
    --muse-chat-text-primary: #d2d2d2;
    --muse-chat-accent-50: #0780df;
    --muse-chat-foregrounds-25: #858585;
    --muse-chat-foregrounds-100: #d2d2d2;
    --muse-chat-foregrounds-200: #eeeeee;
    --muse-chat-backgrounds-50: #202020;
    --muse-chat-backgrounds-100: #2A2A2A;
    --muse-chat-backgrounds-200: #383838;
    --muse-chat-backgrounds-300: #424242;
    --muse-chat-color-blue: #46adff;
    --muse-chat-color-pink: #d33c79;
    --muse-chat-color-user-message: #441a2b;
    --muse-chat-color-crimson-dark: #FF85AB;
    --muse-chat-header-text-color: #ffffff;
    --muse-chat-header-text-strong: #ffffff;
    --muse-chat-experimental-bg: #191919;
    --muse-chat-experimental-border: #F4BC02;
    --muse-chat-hover: #585858;
    --muse-chat-drop-zone-bg: #383838;
    --muse-chat-user-message-bg: var(--muse-chat-backgrounds-100);
    --muse-chat-panel-bg: var(--muse-chat-backgrounds-50);
    --muse-chat-source-border-color: var(--muse-chat-backgrounds-100);
    --muse-chat-inspiration-button-border: var(--muse-chat-backgrounds-200);
    --muse-chat-inspiration-button-border-hover: #858585;
    --muse-chat-avatar-bg: var(--muse-chat-backgrounds-50);
    --muse-chat-context-pill-bg: var(--muse-chat-backgrounds-100);
    --muse-chat-disclaimer-text: rgba(255, 255, 255, 0.4);
    --muse-chat-add-context-button-open: #4c607b;
    --muse-chat-prefab-color: #6b8fc4;
    --muse-chat-color-gray: rgba(88, 88, 88, 1);
    --muse-chat-color-focus-blue: rgba(58, 121, 187, 1);
    --muse-chat-action-button-default: #858585;
    --muse-chat-text-action-button-default: #676767;
    --muse-chat-text-focus: #4F657F;

    --muse-chat-primary-200: #383838;
    --muse-chat-primary-300: #595959;
    --muse-chat-primary-400: #858585;
    --muse-chat-primary-1000: #c0c0c0;
    --muse-chat-primary-1300: #ffffff;
    --muse-chat-light-mode-icon-tint: var(--muse-chat-primary-200);

    --muse-agent-action-border: #BDBDBD;
    --muse-agent-selection-empty: #808080;
}

.-muse-chat-panel {
    background-color: var(--muse-chat-backgrounds-200);
}

.muse-chat-icon {
    ---unity-image-tint-color: blue;
}

.mui-icon-empty {
    background-image: url('project://database/Packages/com.unity.muse.chat/Editor/UI/Assets/icons/empty.png');
}

.mui-icon-empty-margin {
    background-image: url('project://database/Packages/com.unity.muse.chat/Editor/UI/Assets/icons/empty.png');
    width: 28px;
    height: 14px;
}

.mui-icon-muse {
    background-image: url('project://database/Packages/com.unity.muse.chat/Editor/UI/Assets/icons/muse.png');
}

.mui-icon-history {
    --unity-image: url('project://database/Packages/com.unity.muse.chat/Editor/UI/Assets/icons/ClockCounterClockwise.png');
}

.mui-icon-copy {
    --unity-image: url('project://database/Packages/com.unity.muse.chat/Editor/UI/Assets/icons/CopySimple.png');
}

.mui-icon-cmd-run {
    background-image: url('project://database/Packages/com.unity.muse.chat/Editor/UI/Assets/icons/CmdRun.png');
}

