<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xmlns="UnityEngine.UIElements" editor-extension-mode="False">
    <ui:VisualElement>
        <ui:VisualElement name="historyPanelHeaderRoot">
            <ui:Label name="historyPanelHeaderText" text="Date" class="mui-chat-label"/>
        </ui:VisualElement>
        <ui:VisualElement name="historyPanelElementConversationRoot">
            <ui:Label text="New conversation" name="historyPanelElementConversationText" class="mui-chat-label"/>
            <ui:TextField name="historyPanelElementConversationEditText" />
            <ui:VisualElement class="mui-history-panel-entry-controls">
                <ui:Button name="historyPanelFavoriteStateButton">
                    <ui:Image name="historyPanelFavoriteStateIcon" class="mui-action-button-image mui-icon-star"/>
                </ui:Button>
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
