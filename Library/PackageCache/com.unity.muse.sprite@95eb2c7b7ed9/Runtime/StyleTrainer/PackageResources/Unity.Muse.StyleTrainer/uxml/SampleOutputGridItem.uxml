<?xml version="1.0" encoding="utf-8"?>
<ui:UXML
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ui="UnityEngine.UIElements"
        xmlns:appui="Unity.Muse.AppUI.UI"
        xmlns:styleTrainer="Unity.Muse.StyleTrainer"
        xsi:noNamespaceSchemaLocation="../../../../../../../UIElementsSchema/UIElements.xsd"
>
<Style src="../uss/SampleOutputGridItem.uss"/>
<styleTrainer:SampleOutputGridItem class="styletrainer-sampleoutputgriditem" name="SampleOutputGridItem">
    <styleTrainer:PreviewImage name="PreviewImage" class="styletrainer-sampleoutputgriditem__previewimage"/>
    <appui:TextField name="Prompt" class="styletrainer-sampleoutputgriditem__promptField"
                     placeholder="Enter a sample output prompt..."/>
    <appui:ActionButton name="DeleteButton" class="styletrainer-sampleoutputgriditem__deletebutton" icon="delete"/>
</styleTrainer:SampleOutputGridItem>
</ui:UXML>
